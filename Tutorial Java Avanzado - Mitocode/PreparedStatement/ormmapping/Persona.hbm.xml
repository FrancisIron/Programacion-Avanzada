<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Persona" table="persona" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="nombre" column="nombre" type="string" length="50" not-null="false" lazy="false"/>
		<property name="pass" column="pass" type="string" length="50" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
